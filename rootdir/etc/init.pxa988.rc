#for cts test
    chown system system /sys/devices/virtual/camera/rear/rear_flash
    chmod 0664 /sys/devices/platform/Sec_BattMonitor/power_supply/battery/reset_soc
    chmod 0664 /sys/devices/platform/Sec_BattMonitor/fuelgaic
